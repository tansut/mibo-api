    "url": "https://github.com/PhysioHealth/mibo-api/issues"
  },
  "homepage": "https://github.com/PhysioHealth/mibo-api#readme",
  "devDependencies": {
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-typescript": "^3.1.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "shipit-cli": "^2.0.0",
    "shipit-deploy": "^2.4.0",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.10",
    "typings": "^2.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.2",
