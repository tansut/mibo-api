    "mocha": "^3.2.0",
    "moment": "^2.17.0",
    "mongoose": "^4.7.0",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "nodemailer": "^2.7.0",
    "nodemailer-ses-transport": "^1.5.0",
    "passport": "^0.3.2",
    "stripe": "^4.14.0",
    "validator": "^6.2.0"
  }
}